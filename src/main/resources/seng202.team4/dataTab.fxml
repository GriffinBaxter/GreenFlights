<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="dataTabPane" minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team4.controller.dataTabController">
    <children>
        <Button fx:id="dataTabAddFlightPlannerDataBtn" layoutX="566.0" layoutY="14.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="170.0" text="Add Flight Planner Data" />
        <AnchorPane fx:id="modifyAirlinesPane" layoutX="14.0" layoutY="43.0" prefHeight="406.0" prefWidth="235.0">
            <children>
                <TableView fx:id="dataTabAirlineDataTable" layoutX="5.0" layoutY="52.0" prefHeight="346.0" prefWidth="226.0">
                    <columns>
                        <TableColumn fx:id="dataTabAirlineDataTableAirlineColumn" minWidth="234.0" prefWidth="75.0" text="Airline Data" />
                    </columns>
                </TableView>
                <Button fx:id="dataTabModifyAirlinesBtn" layoutX="5.0" layoutY="14.0" mnemonicParsing="false" text="Modify Airlines" />
                <Button fx:id="dataTabAirlinesAddRawDataBtn" layoutX="139.0" layoutY="14.0" mnemonicParsing="false" text="Add Raw Data" />
            </children>
        </AnchorPane>
        <Label fx:id="dataTabDataActionsLabel" layoutX="20.0" layoutY="11.0" prefHeight="32.0" prefWidth="284.0" text="Modify, Add or Remove Data">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <AnchorPane fx:id="modifyAirportsPane" layoutX="257.0" layoutY="43.0" prefHeight="406.0" prefWidth="235.0">
            <children>
                <TableView fx:id="dataTabAirportDataTable" layoutX="5.0" layoutY="52.0" prefHeight="346.0" prefWidth="226.0">
                    <columns>
                        <TableColumn fx:id="dataTabAirportDataTableAirportColumn" minWidth="234.0" prefWidth="75.0" text="Airport Data" />
                    </columns>
                </TableView>
                <Button fx:id="dataTabModifyAirportsBtn" layoutX="5.0" layoutY="14.0" mnemonicParsing="false" text="Modify Airports" />
                <Button fx:id="dataTabAirportsAddRawDataBtn" layoutX="139.0" layoutY="14.0" mnemonicParsing="false" text="Add Raw Data" />
            </children>
        </AnchorPane>
        <AnchorPane fx:id="modifyRoutesPane" layoutX="501.0" layoutY="43.0" prefHeight="406.0" prefWidth="235.0">
            <children>
                <TableView fx:id="dataTabRouteDataTable" layoutX="5.0" layoutY="52.0" prefHeight="346.0" prefWidth="226.0">
                    <columns>
                        <TableColumn fx:id="dataTabRouteDataTableRouteColumn" minWidth="234.0" prefWidth="75.0" text="Route Data" />
                    </columns>
                </TableView>
                <Button fx:id="dataTabModifyRoutesBtn" layoutX="5.0" layoutY="14.0" mnemonicParsing="false" text="Modify Routes" />
                <Button fx:id="dataTabRoutesAddRawDataBtn" layoutX="139.0" layoutY="14.0" mnemonicParsing="false" text="Add Raw Data" />
            </children>
        </AnchorPane>
      <Button layoutX="20.0" layoutY="461.0" mnemonicParsing="false" onAction="#pressHomeButton" text="Home" />
    </children>
</AnchorPane>
