<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="emissionsTabPane" minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team4.controller.emissionsTabController">
    <children>
        <TextField fx:id="emissionsSearchField" layoutX="14.0" layoutY="61.0" prefHeight="26.0" prefWidth="162.0" promptText="Search" />
        <TableView fx:id="emissionsDataTable" layoutX="14.0" layoutY="94.0" prefHeight="362.0" prefWidth="350.0">
            <columns>
                <TableColumn fx:id="emissionsTabRoutesAndEmissionsColumn" prefWidth="350.0" text="Routes and Emissions" />
            </columns>
        </TableView>
        <AnchorPane fx:id="enivornmentalImpactPane" layoutX="385.0" layoutY="64.0" prefHeight="392.0" prefWidth="350.0">
            <children>
                <Label fx:id="offsetEmissionsLabel" layoutX="95.0" layoutY="14.0" text="Offset Emissions">
                    <font>
                        <Font size="22.0" />
                    </font>
                </Label>
                <Button fx:id="environmentalDonationBtn" layoutX="24.0" layoutY="71.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="142.0" text="Envornmental Donation" />
                <Button fx:id="TreesEquivalentBtn" layoutX="183.0" layoutY="71.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="142.0" text="Trees Equivalent" />
                <Label fx:id="reduceEmissionsLabel" layoutX="95.0" layoutY="195.0" text="Reduce Emissions">
                    <font>
                        <Font size="22.0" />
                    </font>
                </Label>
                <Button fx:id="AlternativeRoutesBtn" layoutX="24.0" layoutY="252.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="142.0" text="Alternative Routes" />
                <Button fx:id="AlternativeAircraftTypeBtn" layoutX="183.0" layoutY="252.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="142.0" text="Alternative Aircraft &#10;&#9;  Types" />
            </children>
        </AnchorPane>
        <Button fx:id="emissionsTabModifyRoutesBtn" layoutX="222.0" layoutY="62.0" mnemonicParsing="false" text="Modify Selected Routes" />
        <Label fx:id="currentEmissionsLabel" layoutX="189.0" layoutY="14.0" text="Current Emissions:">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Label fx:id="currentEmissionsValue" layoutX="381.0" layoutY="14.0" text="val">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Label fx:id="currentEmissionsPostfixLabel" layoutX="430.0" layoutY="14.0" text="CO2">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
      <Button layoutX="14.0" layoutY="461.0" mnemonicParsing="false" onAction="#pressHomeButton" text="Home" />
    </children>
</AnchorPane>
